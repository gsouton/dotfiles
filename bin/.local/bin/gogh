# clone the repo into "$HOME/src/gogh"
mkdir -p "$HOME/.local/share"
cd "$HOME/.local/share"
git clone https://github.com/Gogh-Co/Gogh.git gogh
cd gogh

# necessary in the Alacritty terminal
pip install -r requirements.txt
export TERMINAL=alacritty

# Enter themes dir
cd themes

# install themes
./ayu-dark.sh
